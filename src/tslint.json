{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase",
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "no-unused-expression": false,
        "no-output-on-prefix": false
    }
}
